{
  "name": "eccube-web",
  "author": {
    "name": "Dmitrii Mokienko",
    "email": "dmitrymokienko@gmail.com"
  },
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,jsx,tsx,scss,html,yml,yaml}\"",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^3.0.0",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/bebas-neue": "^5.0.18",
    "@fontsource/roboto-condensed": "^5.0.14",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/x-date-pickers": "^7.12.0",
    "axios": "^1.6.3",
    "date-fns": "^3.6.0",
    "draft-js": "^0.11.7",
    "effector": "^23.1.0",
    "effector-localstorage": "^1.0.0",
    "effector-react": "^23.1.0",
    "i18next": "^23.7.16",
    "jwt-decode": "^4.0.0",
    "nanoid": "^5.0.7",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^14.0.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/draft-js": "^0.11.18",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.2.0"
  }
}
