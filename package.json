    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.1.1"
