    "imask": "^7.6.1",
    "jwt-decode": "^4.0.0",
    "nanoid": "^5.0.7",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^14.1.3",
